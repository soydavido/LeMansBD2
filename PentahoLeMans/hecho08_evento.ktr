    <connection>lemans08</connection>
    <sql>SELECT DISTINCT ca.id_carrera, 
--Datos Ranking
ca.puesto_final posicion, 
CASE 
	WHEN puesto_final > 0 THEN (rd.nro_vueltas * pi.total_km)
	WHEN puesto_final = 0 THEN 0
END kilometraje, rd.id_car_evento as id_ranking, to_date(to_char(ev.fecha,'DD-MM-YYYY'),'DD-MM-YYYY') as fecha,
--Datos Equipo
ca.id_parti_equipo as id_equipo, ca.parti_nro_equipo as nro_equipo,
--Datos Vehiculo 
ve.id_vehiculo, ve.categoria, ve.fabricante_auto, 
'Carrera' as tipo,
--Datos Piloto
pi1.id_piloto, (pi1.identificacion).primer_nombre, (pi1.identificacion).primer_apellido
FROM carreras ca, resumen_datos rd, pistas pi, vehiculos ve, eventos ev, plantillas pl, pilotos pi1
WHERE rd.id_carrera = ca.id_carrera 
AND ca.id_parti_evento_pista = pi.id_pista
AND ve.id_vehiculo = ca.id_parti_vehiculo
AND ev.id_evento = ca.id_parti_evento
--De aqui para abajo
AND pl.id_parti_evento = ca.id_parti_evento
AND pl.id_parti_equipo = ca.id_parti_equipo
AND pl.id_parti_vehiculo = ca.id_parti_vehiculo
AND pl.parti_nro_equipo =  ca.parti_nro_equipo
AND pl.id_piloto = pi1.id_piloto
ORDER BY 1,3 DESC ,2</sql>
